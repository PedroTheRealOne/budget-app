<div class = "container">
  <%= link_to "Some Page", root_path %>
  <h1> Suas transações </h1>
  <table class = "table">
    <thead>
      <tr>
        <th>ID</th>
        <th>Valor</th>
        <th>Descrição</th>
        <th>Tipo</th>
      </tr>
    </thead>
    <tbody>
      <% @transactions.each do |transaction|%>
        <tr>
          <td>transaction.id</td>
          <td>transaction.value</td>
          <td>transaction.descriprion</td>
          <td>transaction.transaction_type_id</td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
